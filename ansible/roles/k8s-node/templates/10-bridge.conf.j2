{
    "cniVersion": "{{ k8s_node_cni_plugins_cni_version }}",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ pod_cidr }}"}]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}